<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="CarSaleManagerSystem.Bean.SoleServiceContent" table="sst_content">
        <composite-id>
            <key-property name="sole_service_id" type="java.lang.Integer"/>
            <key-property name="sole_service_type_id" type="java.lang.Integer"/>
        </composite-id>
        <property name="income" type="java.lang.Integer"/>
        <property name="raw_profit" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>
